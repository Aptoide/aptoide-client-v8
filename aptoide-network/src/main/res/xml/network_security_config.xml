<network-security-config>
  <base-config>
    <trust-anchors>
      <certificates src="@raw/vanilla_cert" />
      <certificates src="system" />
    </trust-anchors>
  </base-config>
  <debug-overrides>
    <trust-anchors>
      <certificates src="user" />
    </trust-anchors>
  </debug-overrides>
</network-security-config>