<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="SkeletonView">
    <attr name="skeleton_shape">
      <enum name="rect" value="0" />
      <enum name="oval" value="1" />
    </attr>
    <attr name="skeleton_color" format="reference|color" />
    <attr name="skeleton_border_color" format="reference|color" />
    <attr name="skeleton_border_thickness" format="dimension" />
    <attr name="skeleton_corner_radius" format="dimension" />
  </declare-styleable>
  <declare-styleable name="DownloadProgressView">
    <attr name="progressDrawable" format="reference" />
    <attr name="isPausable" format="boolean" />
    <attr name="enableAnimations" format="boolean" />
    <attr name="installingText" format="reference" />
    <attr name="pausedText" format="reference" />
    <attr name="downloadingText" format="reference" />
  </declare-styleable>
  <declare-styleable name="AnimatedImageView">
    <attr name="animation" format="reference" />
    <attr name="reverseAnimation" format="reference" />
  </declare-styleable>
  <declare-styleable name="AptoideSwipeRefresh">
    <attr name="progressPrimaryColor" format="reference|color" />
    <attr name="progressSecondaryColor" format="reference|color" />
    <attr name="progressBackgroundColor" format="reference|color" />
  </declare-styleable>
</resources>