<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
  <TextView
      android:id="@+id/eskills_faqs_title"
      style="@style/Aptoide.TextView.Medium.L.White"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="@string/eskills_v2_faqs_title"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      android:layout_marginBottom="24dp"
      />

  <include
      android:id="@+id/eskills_faqs_item_1"
      layout="@layout/eskills_faqs_item"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      app:layout_constraintTop_toBottomOf="@id/eskills_faqs_title"
      />
  <include
      android:id="@+id/eskills_faqs_item_2"
      layout="@layout/eskills_faqs_item"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="16dp"
      app:layout_constraintTop_toBottomOf="@id/eskills_faqs_title"
      />
</LinearLayout>